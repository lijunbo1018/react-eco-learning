{
  "name": "webpack-explore",
  "version": "1.0.0",
  "description": "learn webpack & usage experiment",
  "main": "src/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lijunbo1018/webpack-explore.git"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "start:enterprise": "webpack-dev-server --env.target enterprise",
    "debug": "rm -rf ./output && webpack --watch",
    "debug:enterprise": "rm -rf ./output && webpack --watch --env.target enterprise",
    "build": "rm -rf ./output && webpack",
    "build:enterprise": "rm -rf ./output && webpack --env.target enterprise",
    "stats": "rm -rf ./output && webpack --profile --json > stats.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "webpack"
  ],
  "author": "lijunbo",
  "license": "MIT",
  "theme": "./conf/theme.js",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-import": "^1.1.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-livereload-plugin": "^0.10.0",
    "webpack-merge": "^2.3.1"
  },
  "dependencies": {
    "antd": "^2.6.3",
    "codemirror": "^5.23.0",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "marked": "^0.3.6",
    "react": "^15.4.0",
    "react-codemirror": "^0.3.0",
    "react-dom": "^15.4.0",
    "react-intl": "^2.2.3",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
